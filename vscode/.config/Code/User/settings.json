{
    "files.autoSave": "afterDelay",
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "latex-workshop.latex.outDir": "%DIR%/.aux",
    "python.analysis.autoFormatStrings": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "editor.fontFamily": "'UbuntuMono Nerd Font Mono', 'monospace', monospace",
    "explorer.confirmPasteNative": false,
    "explorer.confirmDragAndDrop": false,
    "jupyter.askForKernelRestart": false,
    "explorer.confirmDelete": false,
    "scm.inputFontSize": 0,
    "terminal.integrated.lineHeight": 1,
    "window.zoomLevel": 1.5,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.fontSize": 16,
    "editor.letterSpacing": -0.01,
    "terminal.integrated.gpuAcceleration": "on",
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "cSpell.language": "en,en-GB",
    "cSpell.userWords": [
        "arcticglow",
        "argmax",
        "argmin",
        "Breiman",
        "chartjs",
        "colorbar",
        "Contiki",
        "denoiser",
        "figsize",
        "frombuffer",
        "GELU",
        "imshow",
        "kfold",
        "logvar",
        "pyaudio",
        "randn",
        "Ricci",
        "sharex",
        "suptitle",
        "unflatten",
        "Unigram",
        "virtualenv",
        "vmin",
        "xlabel"
    ],
    "git.openRepositoryInParentFolders": "never",
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#343644",
        "terminal.ansiRed": "#f06d59",
        "terminal.ansiGreen": "#7ac972",
        "terminal.ansiYellow": "#ffd666",
        "terminal.ansiBlue": "#5ec4ff",
        "terminal.ansiMagenta": "#7764d8",
        "terminal.ansiCyan": "#4fc4af",
        "terminal.ansiWhite": "#c6cad7",
        "terminal.ansiBrightBlack": "#585b70",
        "terminal.ansiBrightRed": "#f38b90",
        "terminal.ansiBrightGreen": "#a6e3a1",
        "terminal.ansiBrightYellow": "#f9e2af",
        "terminal.ansiBrightBlue": "#89cefa",
        "terminal.ansiBrightMagenta": "#baacff",
        "terminal.ansiBrightCyan": "#94e2d5",
        "terminal.ansiBrightWhite": "#ffffff",
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {
                "GTK_PATH": ""
            }
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "rnw2tex",
            "command": "Rscript",
            "args": [
                "-e",
                "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
            ],
            "env": {}
        },
        {
            "name": "jnw2tex",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"tex\")"
            ],
            "env": {}
        },
        {
            "name": "jnw2texminted",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"texminted\")"
            ],
            "env": {}
        },
        {
            "name": "pnw2tex",
            "command": "pweave",
            "args": [
                "-f",
                "tex",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "pnw2texminted",
            "command": "pweave",
            "args": [
                "-f",
                "texminted",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": [
                "--synctex",
                "--keep-logs",
                "--print",
                "%DOC%.tex"
            ],
            "env": {}
        }
    ],
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "go.toolsManagement.autoUpdate": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "python.defaultInterpreterPath": "/bin/python3",
    "makefile.configureOnOpen": true,
    "remote.autoForwardPortsSource": "hybrid",
    "arduino.useArduinoCli": true,
    "idf.hasWalkthroughBeenShown": true,
    "idf.espIdfPath": "/home/ettore/.local/share/esp/v5.5/esp-idf",
    "idf.toolsPath": "/home/ettore/.espressif",
    "idf.gitPath": "git",
    "idf.pythonInstallPath": "/usr/bin/python3",
    "files.associations": {
        "*.env": "env"
    },
    "workbench.colorTheme": "Catppuccin Mocha",
    "python.analysis.typeCheckingMode": "basic",
    "tinymist.exportPdf": "onType",
}